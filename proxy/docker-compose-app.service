[Unit]
Description=Docker Compose Application Service
Requires=docker.service
After=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/srv/docker
ExecStart=/opt/easy-nano-node/proxy/docker-compose up -d
ExecStop=/opt/easy-nano-node/proxy/docker-compose down
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target